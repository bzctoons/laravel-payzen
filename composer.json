{
  "name": "noweh/laravel-payzen",
  "description": "Easily create a Payzen payment form.",
  "keywords": [
    "laravel",
    "payzen"
  ],
  "require": {
    "php": ">=5.4",
    "laravel/framework": "^5 || ^7"
  },
  "require-dev": {
    "orchestra/testbench": "3.8.* || ^4.0 || ^5.0 || ^6.0",
    "phpunit/phpunit": "^8.0 || ^9.0",
    "nunomaduro/larastan": "1.0.x-dev"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Julien Schmitt",
      "email": "jschmitt95@protonmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Noweh\\Payzen\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Noweh\\Payzen\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Noweh\\Payzen\\PayzenServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
